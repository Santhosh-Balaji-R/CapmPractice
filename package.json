{
  "name": "CapmPractice",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@cap-js/hana": "^2",
    "@sap/cds": "^9",
    "@sap/hana-client": "^2.27.23",
    "@sap/xssec": "^4",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2",
    "@sap/cds-dk": "^9",
    "@sap/hdi-deploy": "4.9.5"
  },
  "scripts": {
    "start": "cds-serve",
    "watch": "cds watch",
    "sqlDeploy": "cds deploy --to sqlite",
    "watchHybrid": "cds watch --profile hybrid",
    "hanaDeploy": "cds deploy --to hana",
    "mtaDeploy": "cf deploy mta_archives/CapmPractice_1.0.0.mtar"
  },
  "private": true,
  "cds": {
    "requires": {
      "auth": "xsuaa",
      "db": "hana",
      "connectivity": true,
      "destinations": true
    },
    "hana": {
      "deploy-format": "hdbtable"
    }
  }
}